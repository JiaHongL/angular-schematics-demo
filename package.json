{
  "name": "angular-schematics-demo",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "lib:build": "ng build joe-components --prod",
    "lib:serve": "ng build joe-components --watch",
    "lib:build:all": "npm run lib:build && npm run lib:schematics:build && npm run lib:schematics:postbuild:ng-generate",
    "lib:schematics:build": "tsc -p ./projects/joe-components/tsconfig.schematics.json",
    "lib:schematics:copy:collection": "cd ./projects/joe-components && cpy schematics/collection.json ../../dist/joe-components/schematics",
    "lib:schematics:copy:ng-add:schemas": "cd ./projects/joe-components && cpy --parents schematics/*/schema.json ../../dist/joe-components/",
    "lib:schematics:copy:ng-generate:schemas": "cd ./projects/joe-components && cpy --parents schematics/*/*/schema.json ../../dist/joe-components/",
    "lib:schematics:postbuild:ng-generate": "npm run lib:schematics:copy:ng-generate:schemas && npm run lib:schematics:copy:collection",
    "lib:schematics:test:ng-generate:hello": "npm run lib:build && npm run lib:schematics:build && npm run lib:schematics:postbuild:ng-generate && schematics ./dist/joe-components:hello --project=test-app"
  },
  "private": false,
  "dependencies": {
    "@angular-devkit/schematics": "^11.0.6",
    "@angular/animations": "~11.0.1",
    "@angular/common": "~11.0.1",
    "@angular/compiler": "~11.0.1",
    "@angular/core": "~11.0.1",
    "@angular/forms": "~11.0.1",
    "@angular/platform-browser": "~11.0.1",
    "@angular/platform-browser-dynamic": "~11.0.1",
    "@angular/router": "~11.0.1",
    "cpy-cli": "^3.1.1",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1100.6",
    "@angular/cli": "~11.0.2",
    "@angular/compiler-cli": "~11.0.1",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.1.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^11.0.0",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.0.2"
  }
}
